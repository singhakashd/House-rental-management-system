	<query>
		<sql><![CDATA[select distinct to_char(l.l_enddate,'YYYY') as Sales_Year,p.p_state as Region,
sum(cp.amount_paid) OVER (PARTITION BY to_char(l.l_enddate,'YYYY')) AS Sales_by_region

from customer_payment cp join lease l on  cp.fk_customer_payment_c_id = l.fk_lease_c_id
join property p on l.fk_lease_p_id = p.p_id  order by to_char(l.l_enddate,'YYYY')]]></sql>
	</query>
